#: ../../create_localized_help.py:76 __init__.js:294 __init__.js:301
msgid "Description"
msgstr "Popis"

#: ../../create_localized_help.py:78
msgid "This applet creates a menu with a list of all installed extensions in Cinnamon. From each menu item different tasks can be performed:"
msgstr "Tento aplet vytvoří nabídku se seznamem všech nainstalovaných rozšíření v Cinnamonu. Z každé položky nabídky lze provést různé úkoly: "

#: ../../create_localized_help.py:80
msgid "Enable/Disable the extension"
msgstr "Povolit/zakázat rozšíření"

#: ../../create_localized_help.py:81
msgid "Open the extension's settings page"
msgstr "Otevřít okno nastavení rozšíření"

#: ../../create_localized_help.py:82
msgid "Open the extension's Spices page"
msgstr "Otevřít stránku rozšíření na webu Spices"

#: ../../create_localized_help.py:83
msgid "Open the extension folder"
msgstr "Otevřít složku rozšíření"

#: ../../create_localized_help.py:84
msgid "Open its extension.js file"
msgstr "Otevřít soubor extension.js rozšíření"

#: ../../create_localized_help.py:168
msgid "The following two sections are available only in English."
msgstr "Následující dvě části jsou k dispozici pouze v angličtině."

#: ../../create_localized_help.py:172
msgid "Do not install on any other version of Cinnamon."
msgstr "Neinstalujte v žádné jiné verzi Cinnamonu."

#: ../../create_localized_help.py:178
msgid "Compatibility"
msgstr "Kompatibilita"

#. Define them first before self.get_language_stats() is called so these
