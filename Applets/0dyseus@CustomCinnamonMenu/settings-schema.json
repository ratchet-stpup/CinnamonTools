{
	"head": {
		"type": "header",
		"description": "Most settings are not automatically saved when modified.\nPress any of the \"Click here to save settings\" buttons for the settings to take effect."
	},
	"button0": {
		"type": "button",
		"description": "Click here to save settings",
		"callback": "_hardRefreshAll"
	},
	"head0": {
		"type": "header",
		"description": "Applet settings"
	},
	"pref_overlay_key": {
		"type": "keybinding",
		"description": "Keyboard shortcut to open and close the menu",
		"default": "Super_L::Super_R"
	},
	"pref_use_a_custom_icon_for_applet": {
		"type": "checkbox",
		"default": false,
		"description": "Use a custom icon",
		"tooltip": "Unchecking this allows the theme to set the icon"
	},
	"pref_custom_icon_for_applet": {
		"type": "iconfilechooser",
		"default": "custom-start-here-linux-mint-symbolic",
		"description": "Icon",
		"tooltip": "Select an icon to show in the panel.",
		"dependency": "pref_use_a_custom_icon_for_applet",
		"indent": "true"
	},
	"pref_custom_label_for_applet": {
		"type": "entry",
		"default": "Menu",
		"description": "Custom label",
		"tooltip": "Enter custom text to show in the panel."
	},
	"pref_open_menu_on_hover": {
		"type": "checkbox",
		"default": false,
		"description": "Open the menu on mouse over",
		"tooltip": "Enable opening the menu when the mouse enters the applet."
	},
	"pref_menu_hover_delay": {
		"type": "spinbutton",
		"default": 0,
		"min": 0,
		"max": 1000,
		"step": 50,
		"dependency": "pref_open_menu_on_hover",
		"units": "milliseconds",
		"description": "Menu hover delay:",
		"tooltip": "Delay before the menu is opened on entering the applet."
	},
	"sep1": {
		"type": "separator"
	},
	"head1": {
		"type": "header",
		"description": "Menu settings"
	},
	"pref_display_category_icons": {
		"type": "checkbox",
		"default": true,
		"description": "Show category icons",
		"tooltip": "Choose whether or not to show icons on categories."
	},
	"pref_display_application_icons": {
		"type": "checkbox",
		"default": true,
		"description": "Show application icons",
		"tooltip": "Choose whether or not to show icons on applications."
	},
	"pref_display_fav_box": {
		"type": "checkbox",
		"default": true,
		"description": "Show favorites and quit options",
		"tooltip": "Choose whether or not to show the left pane of the menu."
	},
	"pref_show_places": {
		"type": "checkbox",
		"default": true,
		"description": "Show bookmarks and places",
		"tooltip": "Choose whether or not to show bookmarks and places in the menu."
	},
	"pref_show_recents_button": {
		"type": "checkbox",
		"default": true,
		"description": "Show \"Recent documents\" category",
		"tooltip": "For people who want the \"Recent documents\" category hidden without disabling recent documents globally."
	},
	"pref_max_recent_files": {
		"type": "spinbutton",
		"indent": true,
		"default": 20,
		"min": 5,
		"max": 100,
		"step": 1,
		"units": "documents",
		"description": "Max recent documents:",
		"tooltip": ""
	},
	"pref_enable_autoscroll": {
		"type": "checkbox",
		"default": true,
		"description": "Enable auto-scrolling in application list",
		"tooltip": "Choose whether or not to enable smooth auto-scrolling in the application list."
	},
	"pref_animate_menu": {
		"type": "checkbox",
		"default": false,
		"description": "Animate open/close menu",
		"tooltip": ""
	},
	"pref_show_appinfo_box": {
		"type": "checkbox",
		"default": true,
		"description": "Display application info box",
		"tooltip": ""
	},
	"pref_apps_info_box_alignment_to_the_left": {
		"type": "checkbox",
		"default": false,
		"indent": true,
		"description": "Align application info box text to the left",
		"tooltip": ""
	},
	"pref_fuzzy_search_enabled": {
		"type": "checkbox",
		"default": false,
		"description": "Enable fuzzy search (EXPERIMENTAL)",
		"tooltip": ""
	},
	"pref_disable_new_apps_highlighting": {
		"type": "checkbox",
		"default": true,
		"description": "Disable newly installed applications highlighting",
		"tooltip": ""
	},
	"pref_hide_allapps_category": {
		"type": "checkbox",
		"default": false,
		"description": "Hide \"All Applications\" category",
		"tooltip": ""
	},
	"pref_display_favorites_as_category_menu": {
		"type": "checkbox",
		"dependency": "pref_hide_allapps_category",
		"default": false,
		"description": "Display \"Favorites\" as a category menu",
		"tooltip": ""
	},
	"pref_hide_applications_list_scrollbar": {
		"type": "checkbox",
		"default": false,
		"description": "Hide applications list scrollbar",
		"tooltip": ""
	},
	"pref_swap_categories_box": {
		"type": "checkbox",
		"default": false,
		"description": "Swap categories box",
		"tooltip": "By default, the categories box is to the left of the applications list. With this option enabled, the categories box will be placed to the right of the applications list."
	},
	"pref_cat_hover_delay": {
		"type": "spinbutton",
		"default": 0,
		"min": 0,
		"max": 1000,
		"step": 50,
		"units": "milliseconds",
		"description": "Category selection delay:",
		"tooltip": "Delay between switching categories."
	},
	"pref_privilege_elevator": {
		"type": "combobox",
		"default": "gksu",
		"description": "Privileges elevator:",
		"options": {
			"gksu": "gksu",
			"gksudo": "gksudo"
		},
		"tooltip": "The graphical front-end to use to run applications as root.\nThis is used by the \"Open as Root\" context menu and the option \"Gain privileges of not owned files\"."
	},
	"pref_terminal_emulator": {
		"type": "entry",
		"default": "x-terminal-emulator",
		"description": "Terminal emulator:",
		"tooltip": "Choose the terminal emulator used by all launch from terminal options.\nIMPORTANT!!! The terminal emulator has to support the -e argument."
	},
	"sep2": {
		"type": "separator"
	},
	"head3": {
		"type": "header",
		"description": "Quit buttons"
	},
	"pref_system_buttons_display": {
		"type": "combobox",
		"default": 2,
		"description": "Where to display the Quit buttons",
		"options": {
			"Hide all": 0,
			"Inside \"Favorites\" box": 1,
			"Next to Custom launchers box": 2
		}
	},
	"pref_show_lock_button": {
		"type": "checkbox",
		"default": true,
		"description": "Show \"Lock screen\" button",
		"tooltip": ""
	},
	"pref_lock_button_custom_icon": {
		"type": "iconfilechooser",
		"indent": "true",
		"default": "custom-system-lock-screen-symbolic",
		"description": "Custom icon for \"Lock screen\" button:",
		"tooltip": "Only valid if the System buttons box is placed next to the Custom launchers box.\nLeave blank to use default."
	},
	"pref_show_logout_button": {
		"type": "checkbox",
		"default": true,
		"description": "Show \"Logout\" button",
		"tooltip": ""
	},
	"pref_logout_button_custom_icon": {
		"type": "iconfilechooser",
		"indent": "true",
		"default": "custom-system-log-out-symbolic",
		"description": "Custom icon for \"Logout\" button:",
		"tooltip": "Only valid if the System buttons box is placed next to the Custom launchers box.\nLeave blank to use default."
	},
	"pref_show_shutdown_button": {
		"type": "checkbox",
		"default": true,
		"description": "Show \"Shutdown\" button",
		"tooltip": ""
	},
	"pref_shutdown_button_custom_icon": {
		"type": "iconfilechooser",
		"indent": "true",
		"default": "custom-system-shutdown-symbolic",
		"description": "Custom icon for \"Shutdown\" button:",
		"tooltip": "Only valid if the System buttons box is placed next to the Custom launchers box.\nLeave blank to use default."
	},
	"sep3": {
		"type": "separator"
	},
	"head4": {
		"type": "header",
		"description": "Search box"
	},
	"pref_search_filesystem": {
		"type": "checkbox",
		"default": false,
		"description": "Enable file system path entry in search box",
		"tooltip": "Allows path entry in the menu search box."
	},
	"pref_show_search_box": {
		"type": "checkbox",
		"default": true,
		"description": "Show search box. (Needs Cinnamon restart)",
		"tooltip": ""
	},
	"pref_search_box_bottom": {
		"type": "checkbox",
		"default": false,
		"dependency": "pref_show_search_box",
		"description": "Search box at the bottom of menu",
		"tooltip": ""
	},
	"pref_autofit_searchbox_width": {
		"type": "checkbox",
		"default": false,
		"dependency": "pref_show_search_box",
		"description": "Auto-fit the width of the search box with the menu width",
		"tooltip": "If enabled, Custom width for search box will be ignored. Also, this option is ignored if the custom launchers box is placed at either side of the searchbox."
	},
	"pref_custom_width_for_searchbox": {
		"type": "spinbutton",
		"default": 0,
		"min": 0,
		"max": 2048,
		"step": 1,
		"indent": true,
		"units": "pixels",
		"description": "Custom width for search box:",
		"tooltip": "Set to 0 (zero) for not setting a width."
	},
	"sep4": {
		"type": "separator"
	},
	"button2": {
		"type": "button",
		"description": "Click here to save settings",
		"callback": "_hardRefreshAll"
	},
	"head5": {
		"type": "header",
		"description": "Applications context menu extras"
	},
	"pref_show_icons_on_context": {
		"type": "checkbox",
		"default": true,
		"description": "Show icons on context menu"
	},
	"pref_hide_add_to_panel_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Hide \"Add to panel\""
	},
	"pref_hide_add_to_desktop_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Hide \"Add to desktop\""
	},
	"pref_hide_uninstall_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Hide \"Uninstall\""
	},
	"pref_show_run_from_terminal_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Run from terminal\" on context"
	},
	"pref_show_run_from_terminal_as_root_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Run from terminal as root\" on context"
	},
	"pref_show_run_as_root_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Run as root\" on context"
	},
	"pref_show_desktop_file_folder_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Open .desktop file folder\" on context"
	},
	"pref_show_edit_desktop_file_on_context": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Edit .desktop file\" on context",
		"tooltip": "It adds to the applications context menu an entry that will allow you to open the current selected application's .desktop file in a text editor."
	},
	"pref_gain_privileges_on_context": {
		"type": "checkbox",
		"dependency": "pref_show_edit_desktop_file_on_context",
		"default": false,
		"indent": true,
		"description": "Gain privileges of not owned files",
		"tooltip": "If enabled, this options will open as root .desktop files that aren't owned by the current user.\n\nThis isn't needed for all text editors. Some text editors will allow you to open and edit not owned files and will ask for the root password at the moment of saving the changes."
	},
	"pref_custom_editor_for_edit_desktop_file_on_context": {
		"type": "filechooser",
		"dependency": "pref_show_edit_desktop_file_on_context",
		"select-dir": false,
		"default": "",
		"indent": true,
		"description": "Custom text editor:",
		"tooltip": "Set a custom text editor to open .desktop files (just the name of the text editor executable or the path to it).\nLeave it blank to let the system decide."
	},

	"sep5": {
		"type": "separator"
	},
	"head6": {
		"type": "header",
		"description": "Recently used applications"
	},
	"pref_remember_recently_used_apps": {
		"type": "checkbox",
		"default": false,
		"description": "Show \"Recent Applications\" category",
		"tooltip": "From the moment this option is enabled, every application launched from the menu will be stored and displayed in the \"Recent Applications\" category.\nEvery time this option is disabled, the list of recently used applications will be cleared."
	},
	"pref_recent_apps_ignore_favorites": {
		"type": "checkbox",
		"default": false,
		"dependency": "pref_remember_recently_used_apps",
		"description": "Ignore Favorites",
		"tooltip": "If enabled, applications set as Favorite will not be stored in the \"Recent Applications\" category."
	},
	"pref_recently_used_apps_separator": {
		"type": "checkbox",
		"default": false,
		"dependency": "pref_remember_recently_used_apps",
		"description": "Display separator after \"Recent Applications\" category",
		"tooltip": ""
	},
	"pref_recently_used_apps_invert_order": {
		"type": "checkbox",
		"default": false,
		"dependency": "pref_remember_recently_used_apps",
		"description": "Invert recently used applications order",
		"tooltip": "By default, the most recently used application is at the top of the list. With this option enabled, the most recently used application will be at the bottom of the list."
	},
	"pref_recently_used_apps_custom_icon": {
		"type": "entry",
		"dependency": "pref_remember_recently_used_apps",
		"default": "folder-recent",
		"description": "Custom icon for category",
		"tooltip": "Enter custom icon for the \"Recent Applications\" category.\nOnly icon names are allowed."
	},
	"pref_max_recently_used_apps": {
		"type": "spinbutton",
		"dependency": "pref_remember_recently_used_apps",
		"default": 20,
		"min": 5,
		"max": 100,
		"step": 1,
		"units": "applications",
		"description": "Max recently used apps:",
		"tooltip": ""
	},
	"pref_recently_used_apps": {
		"type": "generic",
		"default": []
	},
	"sep6": {
		"type": "separator"
	},
	"head7": {
		"type": "header",
		"description": "Custom launchers"
	},
	"pref_custom_launchers_box_placement": {
		"type": "combobox",
		"default": 4,
		"description": "Custom launchers box placement",
		"options": {
			"Disabled": 0,
			"Left of search box": 1,
			"Right of search box": 2,
			"At the top of menu": 3,
			"At the bottom of menu": 4
		}
	},
	"pref_custom_commands_box_alignment": {
		"type": "combobox",
		"default": 2,
		"description": "Alignment of buttons inside the custom launchers box",
		"options": {
			"Left": 0,
			"Middle": 1,
			"Right": 2
		}
	},
	"button1": {
		"type": "button",
